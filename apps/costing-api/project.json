{
  "name": "costing-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/costing-api",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest build --path apps/costing-api/tsconfig.app.json apps/costing-api/main.ts"
      }
    },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "nest start main.ts",
          "cwd": "apps/costing-api"
        }
      },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "apps/costing-api/jest.config.ts",
        "tsConfig": "apps/costing-api/tsconfig.spec.json"
      }
    }
  }
}
